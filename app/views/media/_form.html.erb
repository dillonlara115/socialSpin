<%= simple_form_for(@medium) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :artist %>
    <%= f.input :artist_description %>
    <%= f.input :album %>
    <%= f.input :album_description %>
    <%= f.input :year %>
    <%= f.input :average_rating %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
