<div class="content-header"><%=image_tag("artists.png", alt: "Artists Image")%>Artists</div>

<% @media.each do |medium| %>
	<div class="media">
	  <a class="pull-left" href="#">
	    <img class="media-object" data-src="holder.js/64x64">
	  </a>
	  <div class="media-body">
	    <h4 class="media-heading"><%= medium.artist %></h4>
	    <!-- Nested media object -->
	    <div class="media">
	     <%= medium.artist_description %>
	     <p>Rate Artist: <%= rating_for medium, "artist" %></p>
	    </div>
	  </div>
	</div>
<% end %>



 