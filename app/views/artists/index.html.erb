<div class="content-header"><%=image_tag("artists.png", alt: "Artists Image")%>Artists</div>

<% @media.each do |medium| %>
	<div class="media">
	  <div class="pull-left">
	    <%= image_tag medium.image_url.to_s, "class" => "media-object", "data-src" => "holder.js/300x200" %>
	  </div>
	  <div class="media-body">
	    <h4 class="media-heading"><%=link_to medium.artist, medium %></h4>
	    <!-- Nested media object -->
	    <div class="media">
	     <%= medium.artist_description %>
	    <h4>Rate Artist :</h4> <%= rating_for medium, "artist" %>
	    </div>

	  </div>
	</div>


<% end %>



 